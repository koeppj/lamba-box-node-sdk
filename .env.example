# Preferred: full Box JWT config JSON (single-line string)
BOX_JWT_CONFIG_JSON=

# Alternative: base64-encoded full Box JWT config JSON
BOX_JWT_CONFIG_JSON_BASE64=

# Alternative: individual JWT fields
BOX_CLIENT_ID=
BOX_CLIENT_SECRET=
BOX_JWT_KEY_ID=
BOX_PRIVATE_KEY=
BOX_PRIVATE_KEY_PASSPHRASE=
BOX_ENTERPRISE_ID=
BOX_USER_ID=

# Optional: make requests as this user
BOX_AS_USER_ID=

# Optional default folder id
BOX_FOLDER_ID=0
